<?xml version="1.0" encoding="ASCII"?>
<componentbasedsystem:ComponentBasedSystemContainer
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:behavioraldescription="platform:/resource/Component-Based-Systems/model/ComponentBasedSystems.ecore#//repository/behavioraldescription"
    xmlns:componentbasedsystem="platform:/resource/Component-Based-Systems/model/ComponentBasedSystems.ecore"
    xmlns:types="platform:/resource/Component-Based-Systems/model/ComponentBasedSystems.ecore#//repository/types">
  <repository>
    <interfaces
        name="HTTP">
      <signatures
          name="HTTPUpload"
          returnType="//@repository/@types.0"/>
      <signatures
          name="HTTPDownload"
          returnType="//@repository/@types.0"/>
    </interfaces>
    <interfaces
        name="Sound">
      <signatures
          name="watermark"
          returnType="//@repository/@types.0"/>
    </interfaces>
    <interfaces
        name="MediaStore">
      <signatures
          name="download"
          returnType="//@repository/@types.0">
        <parameters
            name="param1"
            type="//@repository/@types.0"/>
      </signatures>
      <signatures
          name="upload"
          returnType="//@repository/@types.0"/>
    </interfaces>
    <interfaces
        name="AudioDB">
      <signatures
          name="queryDB"
          returnType="//@repository/@types.0">
        <parameters
            name="param1"
            type="//@repository/@types.1"/>
      </signatures>
      <signatures
          name="addFile"
          returnType="//@repository/@types.2"/>
    </interfaces>
    <components
        name="WebGUI"
        requires="//@repository/@interfaces.2"
        provides="//@repository/@interfaces.0">
      <services
          implements="//@repository/@interfaces.0/@signatures.0">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
        <actions
            xsi:type="behavioraldescription:ExternalCallAction"
            requiredService="//@repository/@interfaces.2/@signatures.0"/>
      </services>
      <services
          implements="//@repository/@interfaces.0/@signatures.1">
        <actions
            xsi:type="behavioraldescription:LoopAction">
          <bodyActions
              xsi:type="behavioraldescription:InternalAction"/>
          <bodyActions
              xsi:type="behavioraldescription:BranchAction">
            <branches
                branchCondition="b&lt;=a">
              <actions
                  xsi:type="behavioraldescription:InternalAction"/>
            </branches>
            <branches
                branchCondition="b>a">
              <actions
                  xsi:type="behavioraldescription:InternalAction"/>
            </branches>
          </bodyActions>
        </actions>
      </services>
    </components>
    <components
        name="MediaManager"
        requires="//@repository/@interfaces.1"
        provides="//@repository/@interfaces.2">
      <services
          implements="//@repository/@interfaces.2/@signatures.0">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
      </services>
      <services
          implements="//@repository/@interfaces.2/@signatures.1">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
      </services>
    </components>
    <components
        name="DBCache"
        requires="//@repository/@interfaces.3"
        provides="//@repository/@interfaces.3">
      <services
          implements="//@repository/@interfaces.3/@signatures.0">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
      </services>
      <services
          implements="//@repository/@interfaces.3/@signatures.1">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
      </services>
    </components>
    <components
        name="DigitalWaterMarking"
        provides="//@repository/@interfaces.1">
      <services
          implements="//@repository/@interfaces.1/@signatures.0">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
      </services>
    </components>
    <components
        name="PoolingAudioDB"
        provides="//@repository/@interfaces.3">
      <services
          implements="//@repository/@interfaces.3/@signatures.1">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
      </services>
      <services
          implements="//@repository/@interfaces.3/@signatures.0">
        <actions
            xsi:type="behavioraldescription:InternalAction"/>
      </services>
    </components>
    <types xsi:type="types:ComplexType"
        name="complexTypeA">
      <members
          name="memberA"
          type="//@repository/@types.0"/>
    </types>
    <types xsi:type="types:SimpleType"
        name="int"
        type="Int"/>
    <types xsi:type="types:Void"
        name="Void"/>
  </repository>
  <environment
      name="mediaFileEnv">
    <container
        name="ApplicationServer"/>
    <container
        name="DatabaseServer"/>
    <linker
        links="//@environment/@container.0 //@environment/@container.1"/>
  </environment>
  <assembly
      name="MediaStoreAssembly">
    <compositeComponents
        name="MediaStoreComposite"
        requires="//@repository/@interfaces.3"
        provides="//@repository/@interfaces.2">
      <childContexts
          instantiates="//@repository/@components.1"
          name="MediaManagerContext">
        <providedRoles
            name="IMediaStore"
            interface="//@repository/@interfaces.2"/>
        <requiredRoles
            name="ISound"
            interface="//@repository/@interfaces.1"/>
        <requiredRoles
            name="IAudioDB"
            interface="//@repository/@interfaces.3"/>
      </childContexts>
      <childContexts
          instantiates="//@repository/@components.3"
          name="DigitalWaterMarkingContext">
        <providedRoles
            name="ISound"
            interface="//@repository/@interfaces.1"/>
      </childContexts>
      <childContexts
          instantiates="//@repository/@components.2"
          name="DBCachContext">
        <providedRoles
            name="IAudioDB"
            interface="//@repository/@interfaces.3"/>
        <requiredRoles
            name="IAudioDB"
            interface="//@repository/@interfaces.3"/>
      </childContexts>
      <assemblyConnectors
          requiringRole="//@assembly/@compositeComponents.0/@childContexts.0/@requiredRoles.1"
          providingRole="//@assembly/@compositeComponents.0/@childContexts.2/@providedRoles.0"/>
      <assemblyConnectors
          requiringRole="//@assembly/@compositeComponents.0/@childContexts.0/@requiredRoles.0"
          providingRole="//@assembly/@compositeComponents.0/@childContexts.1/@providedRoles.0"/>
      <requiredDelegationConnectors
          requiringRole="//@assembly/@compositeComponents.0/@childContexts.2/@requiredRoles.0"/>
      <providedDelegationConnectors
          providingRole="//@assembly/@compositeComponents.0/@childContexts.0/@providedRoles.0"/>
    </compositeComponents>
  </assembly>
  <system name="mediaFileSystem">
    <childContexts
        instantiates="//@repository/@components.0"
        name="WebGUIContext">
      <providedRoles
          name="IHTTP"
          interface="//@repository/@interfaces.0"/>
      <requiredRoles
          name="IMediaStore"
          interface="//@repository/@interfaces.2"/>
    </childContexts>
    <childContexts
        instantiates="//@repository/@components.4"
        name="PoolingAudioDBContext">
      <providedRoles
          name="IAudioDB"
          interface="//@repository/@interfaces.3"/>
    </childContexts>
    <childContexts
        instantiates="//@assembly/@compositeComponents.0"
        name="MediaStoreComposite">
      <providedRoles
          name="IMediaStore"
          interface="//@repository/@interfaces.2"/>
      <requiredRoles
          name="IAudioDB"
          interface="//@repository/@interfaces.3"/>
    </childContexts>
    <assemblyConnectors
        requiringRole="//@system/@childContexts.0/@requiredRoles.0"
        providingRole="//@system/@childContexts.2/@providedRoles.0"
        name="MediaStoreConnector"/>
    <assemblyConnectors
        requiringRole="//@system/@childContexts.2/@requiredRoles.0"
        providingRole="//@system/@childContexts.1/@providedRoles.0"
        name="AudioDBConnector"/>
    <providedDelegationConnectors
        providingRole="//@system/@childContexts.0/@providedRoles.0"
        name="HTTP-ProvidedDelegationConnector"/>
    <allocation
        environment="//@environment">
      <allocationContexts
          container="//@environment/@container.0"
          allocates="//@system/@childContexts.2"/>
      <allocationContexts
          container="//@environment/@container.0"
          allocates="//@system/@childContexts.0"/>
      <allocationContexts
          container="//@environment/@container.1"
          allocates="//@system/@childContexts.1"/>
    </allocation>
  </system>
</componentbasedsystem:ComponentBasedSystemContainer>
